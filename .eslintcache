ò–¥–∏ –¥–æ–º–æ–π</span>{' '}\n          <span role='img' aria-label='happy'>\n            ü§üü•≥\n          </span>\n        </span>\n      )\n    }\n  }\n\n  const repeatingFunc = () => {\n    const whatPairIs = (timetable) => {\n      let shortFormat = 'hh:mm'\n      let pairIndex\n\n      let now = moment()\n      timetable.map((row, index) => {\n        let pairStart = moment(row[0], shortFormat)\n        let pairEnd = moment(row[1], shortFormat)\n        if (now.isBetween(pairStart, pairEnd)) {\n          pairIndex = index\n          return\n        }\n      })\n      return pairIndex\n    }\n\n    let pairIndex = whatPairIs(timetable)\n    let currentPairTime = timetable[pairIndex]\n    if (currentPairTime) {\n      let shortFormat = 'hh:mm'\n      let secondsRemaining = moment\n        .duration(moment(currentPairTime[1], shortFormat).diff(moment()))\n        .asSeconds()\n      const result = moment.utc(secondsRemaining * 1000).format('HH:mm:ss')\n      setResultTime(result)\n      let index = pairIndex\n      let pairName\n      if (dayOfWeek === '–≤–æ—Å–∫—Ä–µ—Å–µ–Ω–∏–µ') {\n        pairName = ': )'\n      } else pairName = items.shedule[parity][dayOfWeek][index]\n      if (result === '00:00:00') {\n        setBreakIndex(pairIndex + 1)\n        setPair(null)\n        playSound()\n      } else {\n        setPair(pairName)\n        setBreakIndex(null)\n      }\n    }\n  }\n\n  useInterval(repeatingFunc, 1000)\n\n  return (\n    <div\n      className='\n    text-5xl items-center lg:items-stretch mt-10 lg:-mt-16 lg:items\n    lg:text-6xl font-bold text-blue flex flex-col lg:justify-end '>\n      <div className='flex items-center lg:justify-between'>\n        <span>{print()}</span>\n        <a\n          className='relative'\n          onClick={() => setShowTooltip((prev) => !prev)}\n          onMouseOver={() => setShowTooltip(true)}\n          onMouseOut={() => setShowTooltip(false)}>\n          <div\n            className={`${\n              showTooltip ? 'absolute' : 'hidden'\n            } bottom-14 left-0 w-auto h-auto bg-admin-blue text-base text-white px-4 py-1 `}>\n            <span>{pair}</span>\n          </div>\n          <svg\n            height='40'\n            width='40'\n            className={`${pair ? '' : 'hidden'} ml-2 z-50 cursor-pointer`}>\n            <circle cx='20' cy='20' r='20' fill='#017383' />\n            <text\n              x='11'\n              y='31'\n              fontFamily='Verdana'\n              fontSize='30'\n              fill='#DDEAEA'>\n              ?\n            </text>\n          </svg>\n        </a>\n      </div>\n\n      <span>{pair && resultTime}</span>\n    </div>\n  )\n}\n","/Users/dh/code/react-shedule-web/src/components/DayCard.jsx",[],"/Users/dh/code/react-shedule-web/src/components/login/UserLoggedIn.jsx",[],"/Users/dh/code/react-shedule-web/src/components/SettingsPopup.jsx",[],"/Users/dh/code/react-shedule-web/src/components/Timetable.jsx",["45","46","47"],"/Users/dh/code/react-shedule-web/src/pages/register.js",[],"/Users/dh/code/react-shedule-web/src/components/Alert.jsx",["48"],{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":67,"column":34,"nodeType":"55","messageId":"56","endLine":67,"endColumn":36},{"ruleId":"53","severity":1,"message":"57","line":72,"column":11,"nodeType":"58","messageId":"59","endLine":72,"endColumn":17},{"ruleId":"60","severity":1,"message":"61","line":112,"column":9,"nodeType":"62","endLine":116,"endColumn":52},{"ruleId":"53","severity":1,"message":"54","line":25,"column":32,"nodeType":"55","messageId":"56","endLine":25,"endColumn":34},{"ruleId":"53","severity":1,"message":"57","line":30,"column":9,"nodeType":"58","messageId":"59","endLine":30,"endColumn":15},{"ruleId":"63","severity":1,"message":"64","line":56,"column":11,"nodeType":"65","messageId":"66","endLine":56,"endColumn":16},{"ruleId":"67","severity":1,"message":"68","line":27,"column":6,"nodeType":"69","endLine